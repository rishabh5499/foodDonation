<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fooddonation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodDonation">
        <activity android:name=".donorEnd"></activity>
        <activity android:name=".waitingPage" />
        <activity
            android:name=".DonationPage"
            android:label="@string/title_activity_donation_page"
            android:theme="@style/Theme.FoodDonation.NoActionBar"></activity>
        <activity android:name=".editProfile" />
        <activity android:name=".auth.signUp" />
        <activity android:name=".landingPage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>